page{
background-color:#eee;
}
.map1{
   border:2rpx solid #fff;
   border-radius: 30%;
}
.form{
  padding:15rpx;
  margin-left:10rpx;
  display: flex;
  flex-direction: column;
}
.topmad{
  height: 90vh;
}
.btn-hover{
  background-color: #21859c;
}
.bottom-button{
  background-color: #41a5bc;
  position: fixed;
  bottom: 0;
  width: 100%;
  text-align: center;
  height:10vh;
  line-height: 8vh;
  color: #fff;
  font-size:16px;
}
.form_title{
  padding-top: 20rpx;
  padding-left:40rpx;
  font-size: 30rpx;
  background-color: #fff;
  font-weight: bolder;
  color: #333;
}
.form_item_title{
  background-color: #fff;
  font-size: 30rpx;
}
.form_item{
  background-color: #fff;
  color:#555;
  font-size:30rpx;
  width:520rpx;
  text-align: right; 
}
.map_item{
  background-color: #fff;
  padding:2%;
  padding-left:40rpx;
  color:#555;
  font-size:30rpx;
}
.selecter{
  width:170rpx;
  text-align: right;
}
.selecter_item{
  text-align: right;
  background-color: #fff;
}
picker{
  background-color: #41a5bc;
}
.submitbtn{
  margin-top:20rpx;
  size: 45rpx;
  background-color:#41a5bc;
  color: #fff;
}
.submitbtn::after {
  border: 0;
  border-radius: 0;
}
.primary{
   color:#41a5bc;
   text-align: right;
}
.selecter_item::after{
  border: 0;
  text-align: right;
}
.newbutton{
  font-size: 30rpx;
  text-align: right;
  margin:0;
  padding: 0;
}
.button{
  font-size: 30rpx;
}

.img{
    background: url('https://upload.cc/i1/2018/06/05/bKyOaB.jpg') no-repeat;
    background-size: contain;
    width:100%;
    height:245rpx;
    background-position: top;
}

